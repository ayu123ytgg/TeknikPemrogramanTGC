from abc import ABC, abstractmethod


class Survey(ABC):
    """
    Kelas abstrak sebagai dasar untuk berbagai jenis survey geofisika
    """
    def __init__(self, nama_survey):
        self.nama_survey = nama_survey

    @abstractmethod
    def processing(self):
        """
        Method abstrak untuk proses pengolahan data
        """
        pass


class SeismicSurvey(Survey):
    def processing(self):
        print(f"[{self.nama_survey}] Proses filtering data gelombang seismik")


class GravitySurvey(Survey):
    def processing(self):
        print(f"[{self.nama_survey}] Proses upward continuation data gravitasi")


class MagnetikSurvey(Survey):
    def processing(self):
        print(f"[{self.nama_survey}] Proses pemetaan anomali magnetik")


# =====================
# Program Utama
# =====================
if __name__ == "__main__":
    daftar_survey = [
        SeismicSurvey("Seismic Survey"),
        GravitySurvey("Gravity Survey"),
        MagnetikSurvey("Magnetik Survey")
    ]

    print("=== Proses Pengolahan Data Survey ===")
    for s in daftar_survey:
        s.processing()
